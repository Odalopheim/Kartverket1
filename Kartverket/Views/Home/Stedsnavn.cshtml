@model List<Kartverket.Models.StedsnavnViewModel>

    <h2>Stedsnavn Resultat</h2>

    @if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <th>Skrivemåte</th>
            <th>Objekttype</th>
            <th>Språk</th>
            <th>Status</th>
        </thead>
        <tbody>
            @foreach (var item in Model)
             {
                 <tr>
                        <td>@item.Skrivemåte</td>
                        <td>@item.Navneobjekttype</td>
                        <td>@item.Språk</td>
                        <td>@item.Navneobjekttype</td>
                </tr>
              }
        </tbody>
    </table>
}
else
{
    <p>Ikke noe resultat.</p>
}